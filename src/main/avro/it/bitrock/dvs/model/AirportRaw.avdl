@namespace("it.bitrock.dvs.model.avro")
protocol AirportRawProtocol {

  record AirportRaw {
    long id;
    string name;
    string codeIata;
    double latitude;
    double longitude;
    string countryName;
    string countryCodeIso2;
    string cityCodeIata;
    string timezone;
    string gmt;
  }

}
