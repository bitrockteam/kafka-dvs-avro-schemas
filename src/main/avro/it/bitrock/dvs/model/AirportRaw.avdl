@namespace("it.bitrock.dvs.model.avro")
protocol AirportRawProtocol {

  record AirportRaw {
    long id;
    string name;
    string iataCode;
    double latitude;
    double longitude;
    string countryName;
    string countryIsoCode2;
    string cityIataCode;
    string timezone;
    string gmt;
