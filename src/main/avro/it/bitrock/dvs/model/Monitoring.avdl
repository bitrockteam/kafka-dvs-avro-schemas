@namespace("it.bitrock.dvs.model.avro.monitoring")
protocol MonitoringProtocol {

  record FlightReceivedListComputationStatus {
    timestamp_ms windowTime;
    timestamp_ms emissionTime;
    timestamp_ms minUpdated;
    timestamp_ms maxUpdated;
    int windowElements = 0;
  }

  record FlightRequestComputationStatus {
    timestamp_ms processingTime;
    timestamp_ms minUpdated;
    timestamp_ms maxUpdated;
    int numErrors;
    int numValid;
    int numInvalid;
  }

}
